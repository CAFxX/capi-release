---
processes:
- name: cc_deployment_updater
  executable: /var/vcap/jobs/cc_deployment_updater/bin/cc_deployment_updater
  env:
    BUNDLE_GEMFILE: /var/vcap/packages/cloud_controller_ng/cloud_controller_ng/Gemfile
    CLOUD_CONTROLLER_NG_CONFIG: /var/vcap/jobs/cc_deployment_updater/config/cloud_controller_ng.yml
    C_INCLUDE_PATH: /var/vcap/packages/libpq/include
    LANG: en_US.UTF-8
    LIBRARY_PATH: /var/vcap/packages/libpq/lib
    NEWRELIC_ENABLE: false
